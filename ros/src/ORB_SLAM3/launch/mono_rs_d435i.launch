<launch>
  <arg name="vocab_path" value="$(find orb_slam3)../../../Vocabulary/ORBvoc.bin" />
  <arg name="config_path" value="$(find orb_slam3)../../../cam_configs/mono_rs_d435i.yaml" />
  <arg name="save_map" value="1" />  <!-- the map is saved at /home/jy/.ros/ -->
  <arg name="odom_scale_factor" value="1"/>
  
  <node name="orb_slam3_mono" pkg="orb_slam3" type="mono" args="$(arg vocab_path) $(arg config_path) $(arg save_map) $(arg odom_scale_factor)" output="screen">
    <remap from="camera/image_raw" to="camera/color/image_raw" />

  </node>

  <!-- start up realsense d435i -->
  <!-- <include file="$(find realsense2_camera)/launch/rs_rgbd.launch" /> -->


</launch>
